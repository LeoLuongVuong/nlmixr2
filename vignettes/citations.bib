@article{mehtaQuantitativeSystemsPharmacology2022,
  title = {Quantitative {{Systems Pharmacology Modeling Framework}} of {{Autophagy}} in {{Tuberculosis}}: {{Application}} to {{Adjunctive Metformin Host-Directed Therapy}}},
